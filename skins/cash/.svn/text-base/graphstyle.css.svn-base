
		.daterange
		
			{
				position: absolute;
				right: -10px;
				top: -15px;
				z-index: 95;
				
									line-height: 16px;
									font-size: 9px;
									font-family: verdana, arial, serif;
									color: #777;
				
				text-shadow: 0px 0px 2px #fff;
			}
			
			#loadingIcon
				{
					position: absolute;
					right: 10px;
					top: 48px;
					z-index: 95;
				}


			.chart_message
				{
				
					display: block;
					margin: auto;
					position: absolute;
					width: 92%;
					height: 80%;
					z-index: 96;
					
					opacity: 0.9;
					
					margin: 10px;
					
					border: 1px solid #fff;
					
					background: #EEEEEE;
					background: -webkit-gradient(radial, 50 400, 2000, 800 200, 40, from(#DDDDDD), to(#EEEEEE));
					
					
						-moz-box-shadow: 0px 0px 25px #333333;
						-webkit-box-shadow: 0px 0px 25px #333333;
						
						-webkit-border-radius: 8px;
						-moz-border-radius: 8px;
				}
				
				.chart_message .closer
				{
					position: absolute;
					right: -15px;
					top: -11px;
				}




			#calendarselect
				{
				
					position: absolute;
					left: 12px;
					z-index: 95;
					top: 20px;
					
				}
				
				

				.chart
					{
						position: relative;
						width: 100%;
						height: 100%;
						
						background: #f8f8f8;
						background: -webkit-gradient(radial, 50 400, 2000, 800 200, 40, from(#666666), to(#FFFFFF));
						
						border-left: 8px solid white;
						border-bottom: 8px solid white;
						padding-bottom: 1px;
						
						
						
						-moz-box-shadow: 0px 0px 2px #292929;
						-webkit-box-shadow: 0px 0px 2px #292929;
					}
					
					.chart .fullbar
						{
							position: relative;
							height: 100%;
							float: left;
							cursor: pointer;
							margin-left: 8px;
							//background: white;	// for testing purposes
							
						}
						
					.chart .fullbar:hover
						{
							z-index: 95;
						}
						
					.chart .fullbar:first-child
						{
							margin: 0;
						}
						
						.chart .fullbar .name
							{
								position: absolute;
								text-align: center;
								top: -12px;
								width: 100%;
							}
						.chart .fullbar .nameoverper
							{
								position: absolute;
								text-align: center;
								top: 22px;
								width: 100%;
							}
						
						.chart .fullbar .spacing
							{
								width: 100%;
							}
							
						.chart .fullbar .claimants
							{		
							
								-webkit-transition-duration : 0.3s; 
								-webkit-transition-timing-function : ease-in-out; 
								-webkit-transition-property:scale, border, z-index;		
								-webkit-transition-delay: 300ms;
								
								text-align: center;					
								width: 100%;
								border-bottom: 1px solid gray;
								border-right: 1px solid white;
								border-left: 1px solid white;
								border-top: 1px solid white;
								z-index: 90;
								margin-left: -1px;
								
								background: -moz-linear-gradient(top, #859fb2, #456c7b);
								background: -webkit-gradient(linear, left top, left bottom, from(#859fb2), to(#456c7b));
							}
							
						.chart .fullbar .claimants:hover
							{								
								background: -moz-linear-gradient(top, #859fd2, #456c9b);
								background: -webkit-gradient(linear, left top, left bottom, from(#859fd2), to(#456c9b));
							
								border-bottom: white 1px solid;
								z-index: 95;
							
								margin-left: 25px;
							
								-webkit-transform: scale(1.5);
								-webkit-border-radius: 4px;
								-webkit-box-shadow: 0px 0px 5px #333;
							}
						
						
						.chart .fullbar .hotkeys
							{
								-webkit-transition-duration : 0.3s; 
								-webkit-transition-timing-function : ease-in-out; 
								-webkit-transition-property:scale, border, z-index;								
								-webkit-transition-delay: 300ms;

								text-align: center;
								width: 100%;
								border-right: 1px solid white;
								border-left: 1px solid white;
 								z-index: 90;
								margin-left: -1px;
								
								background: -moz-linear-gradient(top, #935151, #502c2c);
								background: -webkit-gradient(linear, left top, left bottom, from(#935151), to(#502c2c));
							}
						
						.chart .fullbar .hotkeys:hover
							{
								background: -moz-linear-gradient(top, #b35151, #702c2c);
								background: -webkit-gradient(linear, left top, left bottom, from(#b35151), to(#702c2c));


								border-top: white 1px solid;
								border-bottom: white 1px solid;
								z-index: 95;
							
								margin-left: 25px;
								-webkit-transform: scale(1.5);
								-webkit-border-radius: 4px;
								-webkit-box-shadow: 0px 0px 5px #333;

							}
							
							.chart .fullbar .claimants strong, .chart .fullbar .hotkeys strong, .chart .fullbar .name strong	
								{
									line-height: 13px;
									font-size: 9px;
									font-weight: bold;
									font-family: verdana, arial, serif;
									color: #eee;
									display: none;
									display: block;
									text-shadow: 1px 1px 1px black;
									
								}
								
								
								.chart .fullbar:hover .claimants strong, .chart .fullbar:hover .hotkeys strong, .chart .fullbar .name strong	
								{
									line-height: 13px;
									font-size: 9px;
									font-weight: bold;
									font-family: verdana, arial, serif;
									color: #eee;
									text-shadow: 1px 1px 1px black;
									display: block;
								}
								
								.chart .fullbar .nameoverper strong	
								{
									line-height: 13px;
									font-size: 9px;
									font-weight: bold;
									font-family: verdana, arial, serif;
									color: #eee;
									text-shadow: 1px 1px 1px black;
									display: block;
								}
								
								
							.chart .fullbar .name strong
								{
									line-height: 10px;
									font-size: 10px;
									color: #777;
									text-shadow: 1px 1px 1px #ddd;
								}
								
								
								
							.chart .fullbar:hover .name strong
								{
									line-height: 10px;
									font-size: 10px;
									color: #222;
									text-shadow: 1px 1px 1px white;
								}